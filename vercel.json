{
  "version": 2,
  "builds": [
    {
      "src": "/api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/api-docs",
      "dest": "/api/server.js"
    }
  ]
}
